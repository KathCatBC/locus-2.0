<!-- notes\show_notes_view.handlebars -->
 
    <div class="panel panel-default">
      <div class="panel-heading">
        <h3>Field Notes</h3>

      </div>
      <div class="panel-body">
        <table class="table table-striped table-hover ">
          <thead>
            <tr>
              <th>Contributor</th>
              <th>Link</th>
              <th>Title</th>
              <th>Description</th>
              <th>Entry Date</th>
               <th> </th>
            </tr>
          </thead>
          <tbody>
          {{#each data}}
             

              <tr>
              <td>{{this.Student.username}}</td>
              <td><a href="#"><span class="glyphicon glyphicon-{{doctype}}"></span></a></td>
              <td> {{this.title}}</td>
              <td> {{this.description}}</td>
              <td> {{this.notedate}}</td>
               <td> 
               {{#if this.showDeleteBtn}}
                 <form action="/notes/delete/{{this.id}}" method="GET">
                  
                     <button type="submit">Delete</button>
                   </form>
                  {{/if}}
                </td>
            </tr>
          {{/each}}
            
          </tbody>
        </table> 

      </div>
    </div>
 
    
  
  
