<!-- students\project-view.handlebars -->
  {{> nav/loggedinstudent}}
  <div class="container">
    <div class="panel panel-default">
      <div class="panel-heading">
      <div class="row">
        <div class="col-md-10">
          <h2>Leader Board</h2>
        </div>
        <div class="col-md-2">
          
        </div>
      </div> 
      </div>
      <div class="panel-body">
        <table class="table table-striped table-hover "> 
         <thead>
             <tr>
              <th class='col-md-1'>
              <th class='col-md-2'> <b>Month/Year</b></th>
              <th class='col-md-2'> <b>Project Name</b></th>
              <th class='col-md-2'> <b>User</b></th>
              <th class='col-md-2'> <b>Entries</b></th>
            </tr>
         </thead>
          <tbody>

          {{#each data}}
            <tr>
              <td class='col-md-1'>
              <td class='col-md-2'> {{this.mname}} {{this.yr}}</td>
              <td class='col-md-2'> {{this.pname}}</td>
              <td class='col-md-2'> {{this.uname}}</td>
              <td class='col-md-2'> {{this.projCt}}</td>
            </tr>
          {{/each}}

          </tbody>
        </table> 
      
      </div>
    </div>
  </div>
 
    
  
  