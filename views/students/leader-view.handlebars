<!-- students\project-view.handlebars -->
  {{> nav/loggedinstudent}}
    <div class="panel panel-default">
      <div class="panel-heading">
      <div class="row">
        <div class="col-md-10">
          <h2>Leaders</h2>
        </div>
        <div class="col-md-2">
          
        </div>
      </div> 
      </div>
      <div class="panel-body">
        <table class="table table-striped table-hover "> 
         <thead>
             <tr>
              <th class='col-md-1'>
              <th class='col-md-2'> Project Name</th>
              <th class='col-md-2'> User</th>
              <th class='col-md-2'> Entries</th>
              <th class='col-md-2'> Month/Year</th>
            </tr>
         </thead>
          <tbody>

          {{#each data}}
            <tr>
              <td class='col-md-1'>
              <td class='col-md-2'> {{this.pname}}</td>
              <td class='col-md-2'> {{this.uname}}</td>
              <td class='col-md-2'> {{this.projCt}}</td>
              <td class='col-md-2'> {{this.mname}}/{{this.yr}}</td>
            </tr>
          {{/each}}

          </tbody>
        </table> 
      </div>
    </div>
 
    
  
  