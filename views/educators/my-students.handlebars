<!-- educators\my-students.handlebars -->
{{> nav/loggedineducator}}

<div class="container">
  <div class="panel panel-default">
    <div class="panel-heading">
      <h3>{{data.project.name}}</h3>
    </div>
    <div class="panel-body">
      <table class="table table-striped table-hover ">
        <thead>
          <tr>
            <th><b>Username</b></th>
            <th><b>Link to Student Data</b></th>
            <th><b>Email</b></th>
          </tr>
        </thead>
		    <tbody>
		    	{{#each data.students}}
			      <tr>
			        <td><a href="/educator/student-data/{{id}}/{{project}}" >{{username}}</a></td>
			        <td><a href="/educator/student-data/{{id}}/{{project}}"><span class="glyphicon glyphicon-file"></span></a></td>
			        <td>{{email}}</td>
			      </tr>
		      {{/each}}
		    </tbody>
		  </table> 
		</div>
	</div>
</div>


